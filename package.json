{
  "name": "@shelf/pspdfkit-ssr",
  "version": "3.0.0",
  "description": "Utilities to work with PSPDFKit's server-side rendering",
  "keywords": [
    "pspdfkit",
    "ssr"
  ],
  "repository": "shelfio/pspdfkit-ssr",
  "license": "MIT",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "main": "lib",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib/ && yarn build:types && babel src --out-dir lib --ignore '**/*.test.ts' --extensions '.ts' && find ./lib -name '*.test.d.ts' -delete",
    "build:types": "tsc --emitDeclarationOnly --declaration --isolatedModules false --declarationDir lib",
    "coverage": "jest --coverage",
    "lint": "eslint . --fix",
    "prepack": "yarn build",
    "test": "jest src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "lint-staged": {
    "*.{html,md,yml}": [
      "prettier --write"
    ],
    "*.{js,ts,json}": [
      "eslint --fix"
    ]
  },
  "babel": {
    "extends": "@shelf/babel-config/backend"
  },
  "prettier": "@shelf/prettier-config",
  "jest": {
    "testEnvironment": "node"
  },
  "dependencies": {
    "form-data": "4.0.0",
    "jsonwebtoken": "8.5.1",
    "ms": "3.0.0-canary.1",
    "node-fetch": "2.6.7"
  },
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.19.1",
    "@shelf/babel-config": "1.2.0",
    "@shelf/eslint-config": "2.22.0",
    "@shelf/prettier-config": "1.0.0",
    "@shelf/tsconfig": "0.0.9",
    "@types/jest": "29.0.3",
    "@types/jsonwebtoken": "8.5.9",
    "@types/node": "16",
    "@types/node-fetch": "2.6.2",
    "eslint": "8.24.0",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.8.3"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
